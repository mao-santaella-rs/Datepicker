{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"rules": {
		"vue/order-in-components": 0,
		"linebreak-style": [
			"error",
			"windows"
		],
		"no-console": [
			"error",
			{ 
				"allow": [
					"warn",
					"error",
					"log"
				]
			}
		]
	}
}